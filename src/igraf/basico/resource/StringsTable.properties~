igraf = iGraf: Gráficos Interativos na Internet
arquivo = Arquivo

# GERAL
RESET = reset
ATUALIZAR = Atualizar

# CORES PADRÃO
AZUL = Azul
PRETO = Preto
MARINHO = Marinho
VERMELHO = Vermelho
LARANJA = Laranja
ROXO = Roxo
CINZA_ESCURO = Cinza escuro
BORDO = Bordô
MARROM = Marrom

# DESENHO
DESENHO_INDIVIDUAL = desenhar gráfico da função f(x) =
DESENHO_MULTIPLO = desenhar todos os gráficos editados
ESCONDER_ULTIMO_GRAFICO = ocultar o último gráfico desenhado
ESCONDER_TODOS_GRAFICOS = ocultar todos os gráficos
DELETAR_GRAFICO_SELECIONADO = remover gráfico da função f(x) =
DELETAR_TODOS_GRAFICOS = deletar todos os gráficos editados
EDITAR_FUNCAO = mudar definição de funcao
INSERIR_ABA = inserir mais uma área de desenho

# POLIGONO
POLIGONO = criar polígono
UNDO_POLIGONO = desfazer polígono
REDO_POLIGONO = refazer polígono

# ANIMACAO
ANIMACAO = iniciar/parar animação
RASTRO_NA_TELA = exibir/ocultar rastro da animação na tela
AUMENTAR_VELOCIDADE_ANIMACAO = aumentar velocidade da animação
DIMINUIR_VELOCIDADE_ANIMACAO = diminuir velocidade da animação

# EIXOS
EIXOS = ocultar/exibir eixos coordenados
ESCALA = ocultar/exibir escala
GRADE = ocultar/exibir grade
INSERE_TEXTO = inserir texto
EDITA_TEXTO = editar texto
ATUALIZA_POSICAO_TEXTO = atualização da posição do texto
APAGA_TEXTO = remove texto
DESLOCAR_ORIGEM = deslocar origem

# CALCULO
TRACAR_DERIVADA = traçar gráfico da função derivada de f(x) =
TRACAR_TANGENTE = desenhar reta tangente ao ponto
CALCULAR_INTEGRAL = calcular integral definida
INT_INDEFINIDA = traçar gráfico da antiderivada de f(x) =

# ZOOM
MAIS_ZOOM = aumentar zoom
MENOS_ZOOM = diminuir zoom
ZOOM_PADRAO = restaurar tela padrão

# FINALIZAÇÃO
FIM = Fim do Histórico

